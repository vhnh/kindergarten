{
    "name": "vhnh/kindergarten",
    "license": "MIT",
    "description": "Add an age verification guard to your apps",
    "homepage": "http://github.com/vhnh/kindergarten",
    "authors": [
        {
            "name": "Tobias Thaden",
            "email": "hello@tobiasthaden.com"
        }
    ],
    "support": {
        "issues": "https://github.com/vhnh/kindergarten/issues",
        "source": "https://github.com/vhnh/kindergarten"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Vhnh\\Kindergarten\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vhnh\\Kindergarten\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2.5",
        "ua-parser/uap-php": "^3.9"
    },
    "require-dev": {
        "orchestra/testbench": "^5.3"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
